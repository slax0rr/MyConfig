!! Set the font to PragmataProMono.
*font                          : PragmataProMono:size=10:antialias=true:autohint=false
dmenu.font                     : xft:PragmataProMono:size=10:antialias=true:autohint=false

!xterm
XTerm*.allowBoldFonts          : true
XTerm*.bellIsUrgent            : true
XTerm*.cutNewline              : true
XTerm*.faceName                : PragmataProMono:size=10:antialias=true:autohint=false
XTerm*.fullscreen              : never
XTerm*.internalBorder          : 0
XTerm*.renderFont              : true
XTerm*.termName                : xterm-256color
XTerm*.utf8                    : always
XTerm*.utf8Fonts               : always
XTerm*.visualBell              : false
XTerm*.visualBellLine          : false
XTerm*pointerColor             : white
XTerm*pointerColorBackground   : black
XTerm*selectToClipboard        : true
XTerm*on3Clicks                : regex [^ \n]+
XTerm*SaveLines                : 10000
XTerm*ScrollBar                : false
XTerm*ScrollKey                : true
XTerm*cursorBlink              : false
XTerm*scaleHeight              : 1.05
XTerm*eightBitInput            : false

!uxterm
UXTerm*.allowBoldFonts          : true
UXTerm*.bellIsUrgent            : true
UXTerm*.cutNewline              : true
UXTerm*.faceName                : PragmataProMono:size=10:antialias=true:autohint=false
UXTerm*.fullscreen              : never
UXTerm*.internalBorder          : 0
UXTerm*.renderFont              : true
UXTerm*.termName                : xterm-256color
UXTerm*.utf8                    : always
UXTerm*.utf8Fonts               : always
UXTerm*.visualBell              : false
UXTerm*.visualBellLine          : false
UXTerm*pointerColor             : white
UXTerm*pointerColorBackground   : black
UXTerm*selectToClipboard        : true
UXTerm*on3Clicks                : regex [^ \n]+
UXTerm*SaveLines                : 10000
UXTerm*ScrollBar                : false
UXTerm*ScrollKey                : true
UXTerm*cursorBlink              : false
UXTerm*scaleHeight              : 1.05
UXTerm*eightBitInput            : false

*VT100*translations            : #override Shift <Btn1Up>: exec-formatted("xdg-open '%t'", CLIPBOARD)

