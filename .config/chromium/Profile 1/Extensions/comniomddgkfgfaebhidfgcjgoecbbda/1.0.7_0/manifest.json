{
   "author": "Load Impact",
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon-19.png",
         "38": "images/icon-38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Load Impact User Scenario Recorder"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://api.loadimpact.com; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_appDescription__",
   "homepage_url": "https://loadimpact.com/",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "64": "images/icon-64.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "19",
   "name": "__MSG_appName__",
   "options_page": "options.html",
   "permissions": [ "browsingData", "http://*/*", "https://*/*", "storage", "tabs", "unlimitedStorage", "webRequest", "webRequestBlocking" ],
   "short_name": "__MSG_appShortName__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.7"
}
