--recurse
--languages=php
--exclude=.git
--tag-relative=yes
--PHP-kinds=+cfd-v
--regex-PHP=/^\s*abstract\s+class\s+([^ ]+)/\1/c/
--regex-PHP=/^\s*interface\s+([^ ]+)/\1/c/
--regex-PHP=/^\s*(public\s+|static\s+|abstract\s+|final\s+|protected\s+|private\s+)function\s+\&?\s*([^ (]+)/\2/f/
--regex-PHP=/^\s*function\s+\&?\s*([^ (]+)/\1/f/
--regex-PHP=/^\s*const\s+([^ =]+)/\1/d/
--regex-PHP=/^\s*define\(\"([^ ]+)\"/\1/d/
-f .tags
--format=2
--langmap=php:.inc.php
-h ".inc"
--fields=+aimS
