" Timetac development environment vimrc
if exists('timetacweb')
	cd ~/Development/projects/timetac-web
	call LoadSession()
elseif exists('wrapper')
	cd ~/Development/projects/timetac-wrapper
	call LoadSession()
elseif exists('booking')
    cd ~/Development/vagrant/dev/booking-framework.dev/
    call LoadSession()
elseif exists('c3')
    cd ~/Development/vagrant/dev/c3/
    call LoadSession()
endif

" Configure phpqa
let g:phpqa_messdetector_autorun = 0
let g:phpqa_codesniffer_autorun = 0
let g:phpqa_codecoverage_autorun = 0
let g:phpqa_open_loc = 0
